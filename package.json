{
  "name": "wo-library",
  "version": "1.0.1",
  "type": "module",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "description": "A component library smooth on the edges",
  "keywords": [
    "wo-library",
    "components",
    "react"
  ],
  "scripts": {
    "analyze": "source-map-explorer 'lib/**/*.js' --no-border-checks",
